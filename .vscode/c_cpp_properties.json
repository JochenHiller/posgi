{
  "configurations": [
      {
        "name": "Linux",
        "includePath": [
            "${workspaceFolder}/framework/include",
            "${workspaceFolder}/third_party/plog-1.1.9/include",
            "${workspaceFolder}/third_party/googletest-1.13.0/googletest/include",
            "${workspaceFolder}/third_party/boost_1_82_0"
          ],
          "defines": [],
          "compilerPath": "/usr/bin/gcc", // Path to the C++ compiler
          "cStandard": "c11", // C language standard
          "cppStandard": "c++17", // C++ language standard
